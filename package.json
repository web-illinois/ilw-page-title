{
    "name": "@illinois-toolkit/ilw-page-title",
    "description": "Illinois Toolkit: provides a background image and gradient overlay options for the text of the title of a particular web page.",
    "repository": "github:web-illinois/ilw-page-title",
    "private": false,
    "license": "MIT",
    "version": "1.1.0",
    "type": "module",
    "files": [
        "src/**",
        "dist/**",
        "builder/**"
    ],
    "exports": {
        ".": {
            "import": "./dist/ilw-page-title.js",
            "require": "./dist/ilw-page-title.cjs",
            "default": "./dist/ilw-page-title.js"
        },
        "./ilw-breadcrumbs.css": {
            "import": "./dist/ilw-page-title.css"
        }
    },
    "main": "./dist/ilw-page-title.js",
    "module": "./dist/ilw-page-title.js",
    "scripts": {
        "dev": "vite",
        "build": "tsc --noEmit && vite build --config vite.transpile.config.ts --emptyOutDir && vite build --config vite.build.config.ts --emptyOutDir",
        "preview": "vite preview",
        "prepack": "npm run build"
    },
    "dependencies": {
        "lit": "3.1.3"
    },
    "devDependencies": {
        "typescript": "^5.9.2",
        "vite": "^7.1.6",
        "vite-plugin-dts": "^4.5.4"
    }
}
